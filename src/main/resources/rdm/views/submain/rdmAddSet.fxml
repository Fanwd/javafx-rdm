<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<StackPane xmlns="http://javafx.com/javafx"
           xmlns:fx="http://javafx.com/fxml"
           fx:id="rootStackPane"
           fx:controller="com.fwd.rdm.controller.submain.RdmAddSetController">
    <VBox spacing="10" alignment="CENTER_LEFT">
        <padding>
            <Insets top="30" bottom="30" left="30" right="30"></Insets>
        </padding>
        <Label text="Value:"/>
        <TextArea fx:id="valueTextArea"/>
        <HBox spacing="20" alignment="CENTER_RIGHT">
            <Button text="Add" onAction="#add" prefWidth="100"></Button>
            <Button text="Cancel" onAction="#cancel" prefWidth="100"></Button>
        </HBox>
    </VBox>
</StackPane>
